{
  "name": "react-hot-boilerplate",
  "version": "0.1.2",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "watch:sw": "webpack --config webpack.config.sw.js --watch",
    "build": "npm run build:sw && npm run build:app",
    "build:sw": "webpack --config webpack.config.sw.js",
    "build:app": "webpack --config webpack.config.app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Dan Abramov <dan.abramov@me.com> (http://github.com/gaearon)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/react-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-hot-boilerplate",
  "devDependencies": {
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "css-loader": "*",
    "jsx-loader": "*",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "dependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "localforage": "*",
    "lodash": "^4.17.4",
    "react": "*",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "*",
    "redux-offline": "*",
    "uuid": "^3.0.1"
  }
}
